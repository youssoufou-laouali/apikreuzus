// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id    Int   @id @default(autoincrement())
  name  String
  lastName  String
  email String @unique
  title String
  begin String
  end   String
  description String
  rappel  String
  
}

generator client {
  provider = "prisma-client-js"
}
